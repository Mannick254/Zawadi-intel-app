{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": "../public/**" }
    }
  ],
  "routes": [
    {
      "src": "/",
      "headers": { "Location": "/home" },
      "status": 301
    },
    {
      "src": "/home",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "server/index.js"
    }
  ],
  "cleanUrls": true
}